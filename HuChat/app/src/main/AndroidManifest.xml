<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.vnbamboo.huchat">
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/roundiconhuchat"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/squareiconhuchat"
        android:theme="@style/AppTheme">
        android:supportsRtl="true"
        <activity android:name=".LoginActivity" android:noHistory="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity">
        </activity>
        <activity android:name=".ChatActivity">
        </activity>
        <activity android:name=".CreateNewMessageActivity"></activity>
    </application>

</manifest>